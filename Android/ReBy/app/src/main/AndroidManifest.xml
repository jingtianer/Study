<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reby">
    <!-- if you want to load images from a file OR from the internet -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_login_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_login_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".main.fragment.user.activity.setting.SettingActivity"></activity>
        <activity android:name=".main.fragment.message.activity.MessageDetailActivity" />
        <activity android:name=".main.fragment.home.activity.lookactivity.activity.GoodsIntroActivity" />
        <activity android:name=".main.fragment.user.activity.collection.MyCollectionActivity" />
        <activity android:name=".main.fragment.user.activity.order.activity.MyOrderActivity" />
        <activity android:name=".main.fragment.user.activity.shop.activity.MyShopActivity" />
        <activity android:name=".main.fragment.home.activity.tosellactivity.ToSellActivity" />
        <activity android:name=".main.fragment.home.activity.lookactivity.activity.LookAroundActivity" />
        <activity android:name=".main.fragment.user.activity.detail.UserActivity" />
        <activity android:name=".account.login.activity.LoginActivity" />
        <activity android:name=".welcome.WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".main.activity.MainActivity" />
        <activity android:name=".account.register.activity.RegisterActivity" />
    </application>

</manifest>
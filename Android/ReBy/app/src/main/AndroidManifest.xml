<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.reby">
    <!-- if you want to load images from a file OR from the internet -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_login_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_login_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SettingActivity"></activity>
        <activity android:name=".MessageDetailActivity" />
        <activity android:name=".GoodsIntroActivity" />
        <activity android:name=".MyCollectionActivity" />
        <activity android:name=".MyOrderActivity" />
        <activity android:name=".MyShopActivity" />
        <activity android:name=".ToSellActivity" />
        <activity android:name=".LookAroundActivity" />
        <activity android:name=".UserActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".WelcomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".RegisterActivity" />
    </application>

</manifest>